{
    "id_token": {
        "email": null
    },
    "vp_token": {
        "presentation_definition": {
            "id": "vp token example",
            "input_descriptors": [
                {
                    "id": "id card credential with constraints",
                    "format": {
                        "ldp_ac": {
                            "proof_type": [
                                "CLSignature2019"
                            ]
                        }
                    },
                    "constraints": {
                        "limit_disclosure": "required",
                        "fields": [
                            {
                                "path": [
                                    "$.identifiers.schema_id"
                                ],
                                "filter": {
                                    "type": "string",
                                    "pattern": "3QowxFtwciWceMFr7WbwnM:2:BasicScheme:0.1"
                                }
                            },
                            {
                                "path": [
                                    "$.requested_proof.revealed_attr_groups.ref2.values.given_name"
                                ]
                            },
                            {
                                "path": [
                                    "$.requested_proof.revealed_attr_groups.ref2.values.family_name"
                                ]
                            },
                            {
                                "path": [
                                    "$.requested_proof.revealed_attr_groups.ref2.values.birthdate"
                                ]
                            }
                        ]
                    }
                }
            ]
        }
    }
}